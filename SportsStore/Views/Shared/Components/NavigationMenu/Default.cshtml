@model IEnumerable<string>

<a class="btn btn-block btn-secondary"
   asp-action="List"
   asp-controller="Product"
   asp-route-category="">
    Home
</a>

@foreach (string catgegory in Model)
{
    <a class="btn btn-block 
       @(catgegory == ViewBag.SelectedCategory ? "btn-primary" : "btn-secondary")"
       asp-action="List"
       asp-controller="Product"
       asp-route-category="@catgegory"
       asp-route-productPage="1">
        @catgegory
    </a>
}
